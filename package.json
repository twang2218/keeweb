  "scripts": {
    "start": "grunt",
    "test": "grunt test",
    "postinstall": "cd desktop && $npm_execpath install",
    "build-beta": "grunt --skip-sign --beta && sed 's/<html manifest=\"manifest.appcache\">//' dist/index.html > ../keeweb-beta/index.html && cd ../keeweb-beta && git add index.html && git commit -a -m 'beta' && git push origin master",
    "electron": "electron dist/app",
    "electron:server": "electron dist/app --htmlpath=http://localhost:8085/",
    "dev": "grunt dev --skip-sign",
    "webpack:web": "NODE_ENV=production webpack --config webpack.config.app.js --env.target=web",
    "webpack:web:beta": "NODE_ENV=production webpack --config webpack.config.app.js --env.target=web --env.release=beta",
    "webpack:web:dev": "NODE_ENV=development webpack --config webpack.config.app.js --env.target=web",
    "webpack:web:stats": "NODE_ENV=production webpack --config webpack.config.app.js --env.target=web --env.stats",
    "webpack:desktop:app": "webpack --config webpack.config.app.js --env.target=desktop",
    "webpack:desktop:main": "webpack --config webpack.config.main.js",
    "webpack:desktop:build": "$npm_execpath run webpack:desktop:app && $npm_execpath run webpack:desktop:main",
    "webpack:desktop": "export NODE_ENV=production && $npm_execpath run webpack:desktop:build",
    "webpack:desktop:dev": "export NODE_ENV=development && $npm_execpath run webpack:desktop:build",
    "webpack:desktop:beta": "export NODE_ENV=production && webpack --config webpack.config.app.js --env.target=desktop --env.release=beta && webpack --config webpack.config.main.js",
    "webpack:desktop:stats": "NODE_ENV=production webpack --config webpack.config.app.js --env.target=desktop --env.stats",
    "webpack:cordova": "NODE_ENV=production webpack --config webpack.config.app.js --env.target=cordova",
    "server": "webpack-dev-server --config webpack.config.app.js --env.target=web",
    "server:prod": "NODE_ENV=production webpack-dev-server --config webpack.config.app.js --env.target=web",
    "eslint": "eslint --ignore-path .gitignore ./",
    "clean": "rimraf -r dist",
    "clean:app": "rimraf -r dist/app",
    "clean:build": "rimraf -r dist/build",
    "clean:release": "rimraf -r dist/release",
    "builder:dir": "electron-builder --dir",
    "builder": "electron-builder",
    "builder:all": "$npm_execpath run clean && $npm_execpath run webpack:desktop && $npm_execpath run builder"
  },
  "author": {
